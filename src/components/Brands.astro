            <img src={brand2.src} alt="Apple" class="mx-auto h-24 object-contain">
            <img src={brand3.src} alt="BBVA" class="mx-auto h-24 object-contain">
            <img src={brand4.src} alt="Graphext" class="mx-auto h-24 object-contain">
            <img src={brand5.src} alt="Indra" class="mx-auto h-24 object-contain">
            <img src={brand6.src} alt="Microsoft" class="mx-auto h-24 object-contain">
        </div>
    </div>
</div>
