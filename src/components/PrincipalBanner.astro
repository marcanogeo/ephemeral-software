---
import background from '../assets/bgtech.svg';
import Banner from '../components/BannerCta.astro';

interface Props{
	backgroundImage?: string;
    Title?: string;
    Description?: string;
    ctaText?: string;
}
const props: Props = {
	backgroundImage: background.src,
    Title: "Customized Technology Solutions to Boost Your Business",
    Description: "We develop innovative systems and applications that cater to your unique needs. Our goal is to help you stay ahead of the competition and achieve your business goals.",
    ctaText: "Request a free consultation",

    }
---
<Banner 
	Title={props.Title} 
	Description={props.Description} 
	ctaText={props.ctaText} 
	backgroundImage={background.src}
	/>